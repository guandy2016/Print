      <netMsmqBinding>
        <binding name="MsmgBinding" exactlyOnce="false" maxReceivedMessageSize=" 2147483647" maxBufferPoolSize="2147483647">
          <readerQuotas maxArrayLength="2147483647" maxStringContentLength="2147483647" maxDepth="64"/>
          <security mode="None">
          </security>
        </binding>
      </netMsmqBinding>
    </bindings>
